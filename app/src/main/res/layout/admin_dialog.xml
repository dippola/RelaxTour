<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/dialog_layout"
    android:padding="30dp"
    android:orientation="vertical"
    android:layout_gravity="center_horizontal"
    android:gravity="center_horizontal">
    <Button
        android:id="@+id/admin_dialog0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="신고삭제(하나만)"/>
    <Button
        android:id="@+id/admin_dialog1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="글 보기"/>
    <Button
        android:id="@+id/admin_dialog2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="글 삭제"/>
    <Button
        android:id="@+id/admin_dialog3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="댓글 삭제"/>
    <Button
        android:id="@+id/admin_dialog4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="프로필 수정"/>
    <Button
        android:id="@+id/admin_dialog5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="회원삭제"/>
</LinearLayout>