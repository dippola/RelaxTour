<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/community_detail_image5"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:visibility="gone">

    <ImageView
        android:id="@+id/community_detail_image5_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintDimensionRatio="1:0.6"
        app:layout_constraintEnd_toStartOf="@+id/community_detail_image5_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/community_detail_image5_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="1:0.6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/community_detail_image5_1"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/community_detail_image5_3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintDimensionRatio="1:0.7"
        app:layout_constraintEnd_toStartOf="@+id/community_detail_image5_4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/community_detail_image5_1" />

    <ImageView
        android:id="@+id/community_detail_image5_4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginHorizontal="5dp"
        android:layout_marginTop="5dp"
        app:layout_constraintDimensionRatio="1:0.7"
        app:layout_constraintEnd_toStartOf="@+id/community_detail_image5_5"
        app:layout_constraintStart_toEndOf="@+id/community_detail_image5_3"
        app:layout_constraintTop_toBottomOf="@+id/community_detail_image5_1" />

    <ImageView
        android:id="@+id/community_detail_image5_5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintDimensionRatio="1:0.7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/community_detail_image5_4"
        app:layout_constraintTop_toBottomOf="@+id/community_detail_image5_1" />
</androidx.constraintlayout.widget.ConstraintLayout>